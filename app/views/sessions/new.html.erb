<h1>Sign in</h1>
<%= form_tag sessions_path do %>
  <div class="form-group">
    <%= label_tag :email %><br />
    <%= email_field_tag :email, params[:email], class: 'form-control', placeholder: "Enter email"  %>
  </div>
  <div class="form-group">
    <%= label_tag :password %><br />
    <%= password_field_tag :password, class: 'form-control', placeholder: "Enter Password"  %>
  </div>
  <div class="button form-group"><%= submit_tag "Sign in" %></div>
<% end %>
